{
    "manifest_version": 3,
    "name": "Cookie Getter",
    "version": "1.0",
    "permissions": [
      "cookies",
      "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "/assets/icon.png"
    }
}